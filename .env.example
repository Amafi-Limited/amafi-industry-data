# Server Configuration
PORT=3002
NODE_ENV=development
LOG_LEVEL=info

# Frontend URL for CORS
FRONTEND_URL=http://localhost:5173

# AI API Keys (add your actual keys here)
OPENAI_API_KEY=your_openai_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379

# Other Microservice URLs (for inter-service communication)
# For local development, use localhost URLs
# For production, replace with deployed service URLs
CORPORATE_SERVICE_URL=http://localhost:3001
VALUATION_SERVICE_URL=http://localhost:3003
BUYERS_SERVICE_URL=http://localhost:3004
MATCHING_SERVICE_URL=http://localhost:3005

# CORS Configuration (for production)
ALLOWED_ORIGINS=http://localhost:5173,https://your-frontend-domain.com